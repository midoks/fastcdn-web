#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/midoks/Desktop/mwdev/midoks/fastcdn-web/node_modules/.pnpm/vite@6.3.5_@types+node@22.16.0_jiti@2.4.2_less@4.3.0_sass@1.89.2_terser@5.39.0_yaml@2.8.0/node_modules/vite/bin/node_modules:/Users/midoks/Desktop/mwdev/midoks/fastcdn-web/node_modules/.pnpm/vite@6.3.5_@types+node@22.16.0_jiti@2.4.2_less@4.3.0_sass@1.89.2_terser@5.39.0_yaml@2.8.0/node_modules/vite/node_modules:/Users/midoks/Desktop/mwdev/midoks/fastcdn-web/node_modules/.pnpm/vite@6.3.5_@types+node@22.16.0_jiti@2.4.2_less@4.3.0_sass@1.89.2_terser@5.39.0_yaml@2.8.0/node_modules:/Users/midoks/Desktop/mwdev/midoks/fastcdn-web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/midoks/Desktop/mwdev/midoks/fastcdn-web/node_modules/.pnpm/vite@6.3.5_@types+node@22.16.0_jiti@2.4.2_less@4.3.0_sass@1.89.2_terser@5.39.0_yaml@2.8.0/node_modules/vite/bin/node_modules:/Users/midoks/Desktop/mwdev/midoks/fastcdn-web/node_modules/.pnpm/vite@6.3.5_@types+node@22.16.0_jiti@2.4.2_less@4.3.0_sass@1.89.2_terser@5.39.0_yaml@2.8.0/node_modules/vite/node_modules:/Users/midoks/Desktop/mwdev/midoks/fastcdn-web/node_modules/.pnpm/vite@6.3.5_@types+node@22.16.0_jiti@2.4.2_less@4.3.0_sass@1.89.2_terser@5.39.0_yaml@2.8.0/node_modules:/Users/midoks/Desktop/mwdev/midoks/fastcdn-web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
